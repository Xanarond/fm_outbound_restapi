{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "no-console": "off",
    "camelcase": "off",
    "no-shadow": "warn",
    "max-len": "warn",
    "quotes": "off",
    "arrow-parens": "off",
    "no-return-assign": "off",
    "no-plusplus": "off"
  },
  "ignorePatterns": ["../"]
}
